    "import random as rd\n",
    "import math as m\n",
    "\n",
    "inside = 0\n",
    "total = 0\n",
    "for _ in range(0, 100000000):\n",
    "    x = rd.random()\n",
    "    y = rd.random()\n",
    "    dist = m.sqrt((x*x) + (y*y))\n",
    "    if dist<1:\n",
    "        inside = inside + 1\n",
    "    total = total + 1\n",
    "    \n",
    "print((inside/total)*4)"
   ]
  }
